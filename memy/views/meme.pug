html
    head
        title="Meme"
    style
        include ../public/stylesheets/styleMeme.css
    body
        h1="Ceny mema"
        table
            tr
                th="Ceny"
            each value in prices
                tr 
                    td #{value}
        form(action="/process/" + id, method="post")
            p Podaj nową cenę
            input(type='hidden', name='_csrf', value=csrfToken)
            input(type='number', name='price', required)
            input(type='submit', value='Ok')
        a(href="/") Powrót
            
